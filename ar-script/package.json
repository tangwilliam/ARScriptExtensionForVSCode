{
    "name": "ar-script",
    "displayName": "AR Script",
    "description": "AR Script Coding for VSCode",
    "version": "0.0.2",
    "publisher": "Netease",
    "icon": "images/Insight_Logo.png",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/lua.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository" :
    { 
        "type" : "git" ,
        "url" : "https://github.com/tangwilliam/ARScriptExtensionForVSCode.git"
    }

}
